 ; This programs finds the word that is similar to "queen" but is derived
 ; from a "king" that is not "man" and taking that essence and combining it with "woman".

        ; Initialisation
        mov r1, "king"
        mov r2, "man"
        mov r3, "woman"

        ; Execution
start:  sub r1, r2, r4
        add r4, r3, r5

        ; Validation
        sim r5, "queen", r6
        jlt r6, 95, start

        ; Output
        out r5
