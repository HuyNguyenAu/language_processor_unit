 ; This program loads an external file and compares the two sentences for similarity.

        ; Initialisation
        load r1, "examples/data/load.txt"
        mov r2, "My friends and I enjoy playing soccer together."
        mov r4, "The sentences are similar enough."

        ; Execution
        sim r1, r2, r3
        jgt r3, 80, end
        mov r4, "The sentences are not similar."

        ; Output
end:    out r4